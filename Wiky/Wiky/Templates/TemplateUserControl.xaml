<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Wiky.Templates"
                    xmlns:ViewModels="clr-namespace:ViewModele.ViewModels;assembly=ViewModele"
                    xmlns:userControl="clr-namespace:Wiky.UserControles">

    <DataTemplate DataType="{x:Type ViewModels:ViewModeleDetailArticle}">
        <userControl:detailArticleUserControl/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:ViewModeleListArticle}">
        <userControl:listArticleUserControl/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:ViewModeleAddArticle}">
        <userControl:addArticleUserControl/>
    </DataTemplate>

</ResourceDictionary>